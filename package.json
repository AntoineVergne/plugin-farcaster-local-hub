{
  "name": "@elizaos/plugin-farcaster-local-hub",
  "version": "1.0.0",
  "description": "ElizaOS Farcaster plugin using local Snapchain hub - no Neynar dependency",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "elizaos",
    "eliza",
    "farcaster",
    "snapchain",
    "hub",
    "ai-agent",
    "social",
    "decentralized",
    "self-hosted"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@farcaster/hub-nodejs": "^0.12.7",
    "@noble/hashes": "^1.3.3"
  },
  "peerDependencies": {
    "@elizaos/core": "^1.0.0"
  },
  "devDependencies": {
    "@elizaos/core": "^1.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaos-plugins/plugin-farcaster-local-hub"
  },
  "bugs": {
    "url": "https://github.com/elizaos-plugins/plugin-farcaster-local-hub/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
